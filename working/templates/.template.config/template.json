{
  "$schema": "http://json.schemastore.org/template",
  "author": "PY Rivaille",
  "classifications": [ "ExcelDNA", "F#" ],
  "identity": "WldMr.Excel.ExcelDnaTemplate",
  "description": "An ExcelDNA F# addin SDK project",
  "defaultName": "ExcelDnaFSharpAddin",
  "name": "ExcelDNA F# addin project",
  "shortName": "exceldna-addin",
  "tags": {
    "language": "F#",
    "type": "project"
  },
  "sourceName": "ExcelDnaFSharpAddin",
  "symbols": {
    "Framework": {
      "type": "parameter",
      "description": "The target framework for the project.",
      "datatype": "choice",
      "choices": [
        {
          "choice": "net461",
          "description": "Target net461"
        },
        {
          "choice": "net472",
          "description": "Target net472"
        }
      ],
      "replaces": "net461",
      "defaultValue": "net461"
    },
    "AuthorName": {
      "type": "parameter",
      "defaultValue": "(insert author name)",
      "replaces": "AuthorName",
      "datatype": "text",
      "description": "Author name"
    },
    "Description": {
      "type": "parameter",
      "description": "Description text that will be shown for this nuget package on nuget.org",
      "defaultValue": "(insert description)",
      "replaces": "DescriptionContent"
    }
  }
}